
  <h1>Welcome to aceBook.</h1>

  <%= form_for :user, url: '/users' do |f| %>
    <input type="text" id="First name" name="user_name" placeholder="First Name"><%= f.text_field :first_name %>
    <input type="text" id="Last name" name="user_name" placeholder="Last Name"><%= f.text_field :last_name %>
    <input type="text" id="email" name="user_email" placeholder="Email"><%= f.text_field :email %>
    <input type="text" id="password" name="user_password" placeholder="Password"><%= f.password_field :password %>
    <input type="text" id="passwordconfirm" name="user_confirmpassword" placeholder="Password confirmation"><%= f.password_field :password_confirmation %>
    <%= f.submit "Submit" %>
  <% end %>