<%= form_for @user do | user | %>
<%= render '/shared/signup_error_messages'%>
<%= user.label :name %>
<%= user.text_field :name, placeholder: "name" %>
<%= user.label :email %>
<%= user.email_field :email, placeholder: "email" %>
<%= user.label :password %>
<%= user.password_field :password, placeholder: "password" %>
<%= user.submit "Submit" %>
<% end %>
