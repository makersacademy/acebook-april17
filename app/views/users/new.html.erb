  <h1>Sign Up</h1>
  <%= form_for @user do |f|%>
    <%= f.label :email%><br>
    <%= f.text_field :email%><br>
    <%= f.label :password%><br>
    <%= f.password_field :password%><br>
    <%= f.submit %>
  <%end%>



